		<script src="../../../j_script/checkEmail.js"></script>
		<script language="javascript">
					title();
					function title(){
						autoonclick.dealer_business_web_contact.showTitle(showText);
					}
					function showText(response){
					var show1 = document.getElementById("title");
					show1.innerHTML=response.value;
					}
					
					function doSend(){
					var title;
					var name = document.form1.custname.value;
					var req ="";
					var phone = document.form1.phone.value;
					var callback = document.form1.callback.value;
					var email = document.form1.email.value;
					var more = document.form1.more.value;
					
					for(i=0;i<=2;i++){
						if(document.form1.tit[i].checked){
							title = document.form1.tit[i].value;
						}
					}
					for(i=0;i<=3;i++){
						if(document.form1.req[i].checked){
							req += document.form1.req[i].value + ", ";
						}
					}
					
						if(check_email(email)){
							autoonclick.dealer_business_web_contact.sendEmail(title,name,req,phone,callback,email,more);
							alert('ส่ง Email เรียบร้อย');
						}else{
							alert('Email ไม่ถูกต้อง');
						}
						
					}
					
